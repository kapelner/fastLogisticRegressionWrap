Package: fastLogisticRegressionWrap
Type: Package
Title: Fast Logistic Regression Wrapper
Date: 2023-06-26
Version: 1.0.0
Authors@R: c(
	person("Adam", "Kapelner", email = "kapelner@qc.cuny.edu", role = c("aut", "cre"), comment = c(ORCID = "0000-0001-5985-6792")),
